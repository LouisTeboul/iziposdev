<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.izipass.izipos" version="3.0.5.07032" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>IziPos</name>
    <description>
    </description>
    <author email="izipos.dev@gmail.com" href="http://www.izi-pass.com">
		Izipass
	</author>
    <content src="http://localhost:0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
	<plugin name="cordova-plugin-networkinterface" spec="~1.2.0" />
    <plugin name="cordova-plugin-code-push" spec="^1.11.11" />
    <plugin name="cordova-plugin-file" spec="^5.0.0" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.6.3" />
    <plugin name="cordova-plugin-zip" spec="^3.1.0" />
    <plugin name="cordova-plugin-wkwebview-engine" spec="^1.1.4" />
    <plugin name="cordova-labs-local-webserver" spec="https://github.com/apache/cordova-plugins#local-webserver" />
    <engine name="ios" spec="^4.5.4" />
</widget>
